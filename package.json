{
  "name": "@vector-im/compound-design-tokens",
  "version": "6.7.0",
  "description": "Compound design tokens",
  "sideEffects": false,
  "scripts": {
    "build": "vite build && tsx ./build.ts",
    "sync:icons": "tsx src/syncIcons.ts",
    "dev": "http-server .",
    "dev:colors": "vite --open src/colors/applet.html",
    "format": "yarn exec biome -- format",
    "lint": "yarn exec biome -- lint",
    "check": "yarn exec biome -- check",
    "types": "yarn exec tsc -- --noEmit"
  },
  "keywords": [
    "compound",
    "design tokens",
    "style dictionary",
    "css"
  ],
  "files": [
    "./assets/web/**/*",
    "./icons/**/*"
  ],
  "main": "assets/web/js/index.js",
  "type": "module",
  "exports": {
    "./*": "./*",
    "./assets/web/js": "./assets/web/js/index.js",
    "./assets/web/js/*": {
      "types": "./assets/web/js/*.d.ts",
      "import": "./assets/web/js/*.js"
    },
    "./assets/web/icons": {
      "types": "./assets/web/icons/index.d.ts",
      "import": "./assets/web/icons/index.js",
      "require": "./assets/web/icons/index.cjs"
    },
    "./assets/web/icons/*": {
      "types": "./assets/web/icons/*.d.ts",
      "import": "./assets/web/icons/*.js",
      "require": "./assets/web/icons/*.cjs"
    }
  },
  "author": "",
  "license": "SEE LICENSE IN README.md",
  "homepage": "https://github.com/element-hq/compound-design-tokens",
  "bugs": {
    "url": "https://github.com/element-hq/compound/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/element-hq/compound-design-tokens"
  },
  "devDependencies": {
    "@adobe/leonardo-contrast-colors": "^1.0.0",
    "@babel/core": "^7.24.7",
    "@babel/generator": "^7.24.7",
    "@babel/plugin-transform-react-jsx": "^7.24.7",
    "@babel/types": "^7.24.7",
    "@biomejs/biome": "^2.0.0",
    "@capsizecss/core": "^4.1.2",
    "@capsizecss/metrics": "^3.3.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tokens-studio/sd-transforms": "^2.0.0",
    "@types/babel__core": "^7.20.5",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.0",
    "apca-w3": "^0.1.9",
    "chroma-js": "^3.0.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.3.1",
    "http-server": "^14.1.1",
    "lodash-es": "^4.17.21",
    "prettier": "^3.3.3",
    "style-dictionary": "^5.0.0",
    "svgo": "^4.0.0",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5",
    "undici": "^7.2.0",
    "vite": "^7.1.12",
    "zod": "^4.0.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "@types/react": "*",
    "react": "^17 || ^18 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    }
  }
}
